					}
				],
				"outputs": [
					{
						"referenceName": "SQLTableQ",
						"type": "DatasetReference",
						"parameters": {
							"tablename": "Experience",
							"schemaname": "salesforce"
						}
					}
				]
			}
		],
		"annotations": []
	}
