{
	"name": "pl_dvrxtr_SharepointFileDropByRestApi",
	"properties": {
		"activities": [
			{
				"name": "CALL LogicApps FileDrop",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": {
						"value": "@pipeline().globalParameters.jprLogicApps_FileDropURL",
						"type": "Expression"
					},
					"method": "POST",
					"body": {
						"value": "{\"BlobFilename\":\"@{pipeline().parameters.BlobFileName}\", \"FileSuffix\":\"@{pipeline().parameters.FileSuffix}\", \"BlobPath\": \"@{pipeline().parameters.BlobPath}\", \"DestinationPath\":\"@{pipeline().parameters.DestinationPath}\", \"DestinationSite\":\"@{pipeline().parameters.DestinationSite}\"}",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"BlobPath": {
				"type": "string",
				"defaultValue": "external/xtrjpr"
			},
			"BlobFileName": {
				"type": "string",
				"defaultValue": "BusinessLocation.zip"
			},
			"FileSuffix": {
				"type": "string",
				"defaultValue": "_dev"
			},
			"DestinationSite": {
				"type": "string",
				"defaultValue": "DigitalVisitationRegistrationSystem-DHHS-GRP"
			},
			"DestinationPath": {
				"type": "string",
				"defaultValue": "Shared%20Documents/13.%20Exports/DJPPR-Test"
			}
		},
		"variables": {
			"LogicAppsJson": {
				"type": "String"
			}
		},
		"folder": {
			"name": "DVR Product Dashboard/98 Common"
		},
		"annotations": []
	}
}