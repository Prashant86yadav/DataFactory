{
	"name": "SERP API _Reviews_Test",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"QueryParameters.next_page_token": "$.serpapi_pagination.next_page_token"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['search_metadata']['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['search_metadata']['status']"
								},
								"sink": {
									"name": "status"
								}
							},
							{
								"source": {
									"path": "$['search_metadata']['json_endpoint']"
								},
								"sink": {
									"name": "json_endpoint"
								}
							},
							{
								"source": {
									"path": "$['search_metadata']['created_at']"
								},
								"sink": {
									"name": "created_at"
								}
							},
							{
								"source": {
									"path": "$['search_metadata']['processed_at']"
								},
								"sink": {
									"name": "processed_at"
								}
							},
							{
								"source": {
									"path": "$['search_metadata']['google_maps_reviews_url']"
								},
								"sink": {
									"name": "google_maps_reviews_url"
								}
							},
							{
								"source": {
									"path": "$['search_metadata']['raw_html_file']"
								},
								"sink": {
									"name": "raw_html_file"
								}
							},
							{
								"source": {
									"path": "$['search_metadata']['total_time_taken']"
								},
								"sink": {
									"name": "total_time_taken"
								}
							},
							{
								"source": {
									"path": "$['search_parameters']['engine']"
								},
								"sink": {
									"name": "engine"
								}
							},
							{
								"source": {
									"path": "$['search_parameters']['data_id']"
								},
								"sink": {
									"name": "data_id"
								}
							},
							{
								"source": {
									"path": "$['search_parameters']['hl']"
								},
								"sink": {
									"name": "hl"
								}
							},
							{
								"source": {
									"path": "$['search_information']['reviews_results_state']"
								},
								"sink": {
									"name": "reviews_results_state"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "SerpAPI_test",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSQLSP",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "ACS/MSA"
		},
		"annotations": []
	}
}