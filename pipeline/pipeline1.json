{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "COPY rawbrp",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SalesforceSource",
						"query": {
							"value": "SELECT Id, OwnerId, IsDeleted, Name FROM @{pipeline().parameters.sfTableName}\nWHERE LastModifiedDate > {ts'@{variables('brpExtractFrom')}'}\nAND CreatedDate <= {ts'@{variables('brpExtractTo')}'}'}\n",
							"type": "Expression"
						},
						"readBehavior": "queryAll"
					},
					"sink": {
						"type": "AvroSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "AvroWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ACSDSSF",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_Avro_newDP",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"sfschemaname": {
				"type": "string",
				"defaultValue": "salesforce"
			},
			"sfTableName": {
				"type": "string",
				"defaultValue": "assessment__c"
			},
			"RerunFromDate": {
				"type": "string",
				"defaultValue": "2199-12-31 00:00:00.000"
			},
			"RerunToDate": {
				"type": "string",
				"defaultValue": "2199-12-31 00:00:00.000"
			},
			"ColumnList": {
				"type": "string",
				"defaultValue": "id,name"
			}
		},
		"variables": {
			"brpExtractFrom": {
				"type": "String",
				"defaultValue": "2199-12-31 00:00:00"
			},
			"brpExtractTo": {
				"type": "String",
				"defaultValue": "2199-12-31 00:00:00"
			}
		},
		"folder": {
			"name": "DVR Product Dashboard/98 Common"
		},
		"annotations": []
	}
}