{
	"name": "SF Object Refresh Template",
	"properties": {
		"activities": [
			{
				"name": "Copy Salesforce object",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SalesforceSource",
						"query": "select * from assessment__c\n",
						"readBehavior": "query"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"parallelCopies": 4,
					"dataIntegrationUnits": 16,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ACSDSSFOR",
						"type": "DatasetReference",
						"parameters": {
							"SourceTableName": {
								"value": "@pipeline().parameters.sourcetable",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SQLTableQ",
						"type": "DatasetReference",
						"parameters": {
							"tablename": {
								"value": "@pipeline().parameters.targettable",
								"type": "Expression"
							},
							"schemaname": "salesforce"
						}
					}
				]
			}
		],
		"parameters": {
			"sourcetable": {
				"type": "string",
				"defaultValue": "AccountHistory_s"
			},
			"targettable": {
				"type": "string",
				"defaultValue": " [salesforce].[AccountHistory_s] "
			}
		},
		"variables": {
			"HWM": {
				"type": "String",
				"defaultValue": "2199-12-31 00:00:00"
			}
		},
		"folder": {
			"name": "ACS/Salesforce"
		},
		"annotations": []
	}
}