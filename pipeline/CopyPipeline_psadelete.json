{
	"name": "CopyPipeline_psadelete",
	"properties": {
		"activities": [
			{
				"name": "Copy_psa",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Destination",
						"value": "indextest"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSearchIndexSink",
						"writeBatchSize": 1000,
						"writeBehavior": "merge"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['_id']"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['firstName']"
								},
								"sink": {
									"name": "firstName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['lastName']"
								},
								"sink": {
									"name": "lastName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['isAlive']"
								},
								"sink": {
									"name": "isAlive",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['age']"
								},
								"sink": {
									"name": "age",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['streetAddress']"
								},
								"sink": {
									"name": "streetAddress",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['city']"
								},
								"sink": {
									"name": "city",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['state']"
								},
								"sink": {
									"name": "state",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['postalCode']"
								},
								"sink": {
									"name": "postalcode",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['phoneNumbers'][0]['type']"
								},
								"sink": {
									"name": "phoneNumber",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['phoneNumbers'][0]['number']"
								},
								"sink": {
									"name": "phoneNumber_type",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_psa",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_psa",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}